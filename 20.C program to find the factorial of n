#include <stdio.h>
int main() {
    int i,n, factorial = 1;
    printf("Enter a number: ");
    scanf("%d", &n);
	if(n<0)
    {
    	printf("Number is invalid");
	}	
	else {
    for (i = 1; i <= n; i++) {
        factorial *= i;
    }

    printf("Factorial of %d is %d", n, factorial);
    
}
    return 0;
}
